<html>

<head>
  <meta charset="utf-8">
  <link rel="stylesheet" href="../public/styles.css">
</head>

<body>
  <div class="user-list">
    {{#users}}
    <div class="user-card">
      <img class="user-pic" src={{avatar}}>
      <a href="{{id}}" class="name">{{name}}</a> {{#job}}
      <p class="hired">{{job}}, {{company}}</p>{{/job}} {{^job}}
      <p class="available">Available for Hire</p>{{/job}}
    </div>
    {{/users}}
  </div>
</body>

</html>